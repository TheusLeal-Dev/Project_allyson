      <console name="Connection" type="STANDARD" schema="main" session="Main"><![CDATA[CREATE TABLE usuario (
    id INTEGER PRIMARY KEY AUTOINCREMENT,
    nome TEXT NOT NULL,
    senha TEXT NOT NULL
);





INSERT INTO usuario(nome, senha) VALUES
('aluno', '123'),
('admin', '123'),
('theus', '123');






CREATE TABLE perfil (
    id INTEGER PRIMARY KEY AUTOINCREMENT,
    nome_perfil TEXT
);

CREATE TABLE status_usuario (
    id INTEGER PRIMARY KEY AUTOINCREMENT,
    descricao TEXT
);

CREATE TABLE usuario_perfil (
    id_usuario INTEGER,
    id_perfil INTEGER,
    FOREIGN KEY (id_usuario) REFERENCES usuario(id),
    FOREIGN KEY (id_perfil) REFERENCES perfil(id)
);

CREATE TABLE log_usuario (
    id INTEGER PRIMARY KEY AUTOINCREMENT,
    id_usuario INTEGER,
    acao TEXT,
    data_acao DATETIME DEFAULT CURRENT_TIMESTAMP,
    FOREIGN KEY (id_usuario) REFERENCES usuario(id)
);
 --Trigger Insert
CREATE TRIGGER trg_usuario_insert
AFTER INSERT ON usuario
BEGIN
    INSERT INTO log_usuario (id_usuario, acao)
    VALUES (NEW.id, 'INSERT');
END;



--Trigger Update
CREATE TRIGGER trg_usuario_update
AFTER UPDATE ON usuario
BEGIN
    INSERT INTO log_usuario (id_usuario, acao)
    VALUES (NEW.id, 'UPDATE');
END;



--Trigger Delete
CREATE TRIGGER trg_usuario_delete
AFTER DELETE ON usuario
BEGIN
    INSERT INTO log_usuario (id_usuario, acao)
    VALUES (OLD.id, 'DELETE');
END;



--View
CREATE VIEW vw_usuario_completo AS
SELECT 
    u.id AS usuario_id,
    u.nome AS usuario_nome,
    p.nome_perfil,
    s.descricao AS status,
    l.acao,
    l.data_acao
FROM usuario u
LEFT JOIN usuario_perfil up ON up.id_usuario = u.id
LEFT JOIN perfil p ON p.id = up.id_perfil
LEFT JOIN status_usuario s ON s.id = u.id
LEFT JOIN log_usuario l ON l.id_usuario = u.id;

